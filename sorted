import java.util.*;
 class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int[] b= new int[a];
	    int d=0;
	    for(int i=0;i<a;i++)
	    {
	        b[i]=sc.nextInt();
	    }
	    for(int i=0;i<a-1;i++)
	    {
	        if(b[i]<b[i+1]) d++;
	        else d--;
	    }
	    if(d==a-1) System.out.println("The array is sorted");
	    else System.out.println("The array is unsorted");
	}
}
